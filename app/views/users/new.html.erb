<h1>Sign up</h1>
<%= form_for @user do |f| %>
  <div id="form">
      <%= f.label :username, "Username" %>
      <%= f.text_field :username, id: 'username' %>
      
      <%= f.label :email, "Email" %>
      <%= f.text_field :email, id: 'user_email' %>


      <%= link_to "I do have an account"%>
      <%= f.submit "Register" %>
  </div>
<% end %>
<%= link_to 'Log in', login_path %>